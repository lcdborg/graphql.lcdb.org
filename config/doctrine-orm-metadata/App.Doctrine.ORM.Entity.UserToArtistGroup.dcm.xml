<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Doctrine\ORM\Entity\UserToArtistGroup" table="artist_groups_users_xref">
    <id name="artistGroup" type="integer" association-key="true" column="ref_artist_group">
      <options>
        <option name="unsigned">true</option>
      </options>
    </id>
    <id name="user" type="integer" association-key="true" column="ref_user">
      <options>
        <option name="unsigned">true</option>
      </options>
    </id>
    <field name="ref_user_update" type="integer" nullable="false">
      <options>
        <option name="unsigned">true</option>
      </options>
    </field>
    <field name="temporary" type="boolean" nullable="false"/>
    <many-to-one field="user" target-entity="App\Doctrine\ORM\Entity\User" inversed-by="userToArtistGroups">
      <join-columns>
        <join-column name="ref_user" referenced-column-name="user_key" nullable="false"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="artistGroup" target-entity="App\Doctrine\ORM\Entity\ArtistGroup" inversed-by="userToArtistGroups">
      <join-columns>
        <join-column name="ref_artist_group" referenced-column-name="artist_group_key" nullable="false"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
