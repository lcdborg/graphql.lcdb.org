<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Doctrine\ORM\Entity\InternetArchive\File" table="File_InternetArchive">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="name" type="string" nullable="false"/>
    <field name="body" type="text" nullable="true"/>
    <many-to-one field="identifier" target-entity="App\Doctrine\ORM\Entity\InternetArchive\Identifier" inversed-by="files">
      <join-columns>
        <join-column name="identifier_id" referenced-column-name="id" nullable="false"/>
      </join-columns>
    </many-to-one>
    <options>
      <option name="charset">utf8mb4</option>
      <option name="engine">MyISAM</option>
    </options>
  </entity>
</doctrine-mapping>
