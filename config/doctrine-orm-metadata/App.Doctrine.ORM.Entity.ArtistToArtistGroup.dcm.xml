<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Doctrine\ORM\Entity\ArtistToArtistGroup" table="artist_groups_xref">
    <id name="artistGroup" type="integer" association-key="true" column="ref_artist_group">
      <options>
        <option name="unsigned">true</option>
      </options>
    </id>
    <id name="artist" type="integer" association-key="true" column="ref_artist">
      <options>
        <option name="unsigned">true</option>
      </options>
    </id>
    <field name="reason" type="string" nullable="true"/>
    <many-to-one field="artistGroup" target-entity="App\Doctrine\ORM\Entity\ArtistGroup" inversed-by="artistToArtistGroups">
      <join-columns>
        <join-column name="ref_artist_group" referenced-column-name="artist_group_key" nullable="false"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="artist" target-entity="App\Doctrine\ORM\Entity\Artist" inversed-by="artistToArtistGroups">
      <join-columns>
        <join-column name="ref_artist" referenced-column-name="artist_key" nullable="false"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="user" target-entity="App\Doctrine\ORM\Entity\User" inversed-by="artistToArtistGroups">
      <join-columns>
        <join-column name="ref_user" referenced-column-name="user_key" nullable="false"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
